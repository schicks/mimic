{
  "name": "mimic",
  "displayName": "mimic",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "chatParticipants": [
      {
        "id": "mimic.participant",
        "name": "mimic",
        "fullName": "Mimic",
        "description": "I can mimic your commands!",
        "isSticky": true
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
        "mimic.initialPrompt": {
          "type": "string",
          "default": "You are a rubber duck. No matter what the user says, repeat it back to them, starting now.",
          "description": "The initial prompt for the mimic chat participant."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "test:debug": "vscode-test --debug",
    "test:watch": "npm run watch & vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
